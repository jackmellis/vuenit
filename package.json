{
  "name": "vuenit",
  "version": "0.4.1",
  "description": "Vue Unit Test Helpers for server-side testing",
  "main": "lib/index.js",
  "scripts": {
    "test": "ava",
    "watch": "ava --watch",
    "coverage": "nyc ava",
    "lint": "eslint lib",
    "publish-please": "publish-please",
    "prepublish": "publish-please guard",
    "lcov-fix": "node node_modules/jpex-build-tools/lcov-fix",
    "travis-prepublish": "node node_modules/jpex-build-tools/travis-prepublish"
  },
  "ava": {
    "files": "spec/**/*.spec.js",
    "source": "lib/**/*.js",
    "require": [
      "./spec/helpers/setup.js"
    ],
    "concurrency": 8
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackmellis/vuenit.git"
  },
  "author": "Jack Ellis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackmellis/vuenit/issues"
  },
  "homepage": "https://github.com/jackmellis/vuenit#readme",
  "dependencies": {
    "mock-http-client": "^0.1.0",
    "path-to-regexp": "^1.7.0",
    "vue-inject": "^1.0.0",
    "vue-template-compiler": "^2.2.6"
  },
  "peerDependencies": {
    "vue": "2.x"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "ava-spec": "^1.1.0",
    "browser-env": "^2.0.31",
    "eslint": "^3.19.0",
    "jpex-build-tools": "github:jpex-js/jpex-build-tools",
    "module-alias": "^2.0.0",
    "nyc": "^10.3.2",
    "publish-please": "^2.3.1",
    "require-extension-hooks": "^0.2.0",
    "sinon": "^2.2.0",
    "vue": "^2.2.6",
    "vuex": "^2.3.1"
  }
}
